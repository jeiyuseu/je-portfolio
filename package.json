        "react-dom": "^18.2.0",
        "scrollreveal": "^4.0.9",
        "tailwind-scrollbar": "^3.0.5",
        "vite-plugin-static-copy": "^0.17.0",
        "vite-tsconfig-paths": "^4.2.0"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.4",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
